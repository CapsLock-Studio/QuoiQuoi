<div class='col-xs-12'>
  <div class='page-header page-header-with-buttons'>
    <h1 class='pull-left'>
      <i class='icon-dashboard'></i>
      <span>Dashboard</span>
    </h1>
  </div>
  <div class='alert alert-info alert-dismissable'>
    <a class='close' data-dismiss='alert' href='#'>&times;</a>
    歡迎來到 QuoiQuoi 布知道管理界面
  </div>
  <div class='row'>
    <div class='col-sm-6 box'>
      <div class='box-header'>
        <div class='title'>
          <i class='icon-inbox'></i>
          新訂單&nbsp;<span class="text-success">2個月內</span>
        </div>
        <div class='actions'>
          <a class="btn box-remove btn-xs btn-link" href="#"><i class='icon-remove'></i>
          </a>
                      
          <a class="btn box-collapse btn-xs btn-link" href="#"><i></i>
          </a>
        </div>
      </div>
      <div class='box-content'>
        <div id='stats-chart1' style="height:200px;"></div>
      </div>
    </div>
    <div class='col-sm-6 box'>
      <div class='box-header'>
        <div class='title'>
          <i class='icon-group'></i>
          新用戶註冊&nbsp;<span class="text-success">2個月內</span>
        </div>
        <div class='actions'>
          <a class="btn box-remove btn-xs btn-link" href="#"><i class='icon-remove'></i>
          </a>
                      
          <a class="btn box-collapse btn-xs btn-link" href="#"><i></i>
          </a>
        </div>
      </div>
      <div class='box-content'>
        <div id='stats-chart2' style="height:200px;"></div>
      </div>
    </div>
  </div>
  <hr>
  <div class='row'>
    <div class='col-sm-8'>
      <div class='box'>
        <div class='box-header'>
          <div class='title'>
            <div class='icon-file-text'></div>
            訂單
          </div>
          <div class='actions'>
            <a class="btn box-remove btn-xs btn-link" href="#"><i class='icon-remove'></i>
            </a>
                        
            <a class="btn box-collapse btn-xs btn-link" href="#"><i></i>
            </a>
          </div>
        </div>
        <div class='row'>
          <div class='col-sm-4'>
            <%= link_to check_admin_orders_path do %>
              <div class='box-content box-statistic'>
                <h3 class='title text-info'><%= @order_wait_confirm_remittance %></h3>
                <small>待確認匯款</small>
                <div class='text-info icon-check align-right'></div>
              </div>
            <% end %>
          </div>
          <div class='col-sm-4'>
            <%= link_to deliver_admin_orders_path do %>
              <div class='box-content box-statistic'>
                <h3 class='title text-primary'><%= @order_pre_shipping %></h3>
                <small>待出貨訂單</small>
                <div class='text-primary icon-truck align-right'></div>
              </div>
            <% end %>
          </div>
          <div class='col-sm-4'>
            <%= link_to closed_admin_orders_path do %>
              <div class='box-content box-statistic'>
                <h3 class='title text-success'><%= @closed_order %></h3>
                <small>已完成訂單</small>
                <div class='text-success icon-flag align-right'></div>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
    <div class='col-sm-4'>
      <div class='box'>
        <div class='box-header'>
          <div class='title'>
            <div class='icon-tasks'></div>
            客製化訂單
          </div>
          <div class='actions'>
            <a class="btn box-remove btn-xs btn-link" href="#"><i class='icon-remove'></i>
            </a>

            <a class="btn box-collapse btn-xs btn-link" href="#"><i></i>
            </a>
          </div>
        </div>
        <div class='row'>
          <div class='col-sm-12'>
            <%= link_to check_admin_order_custom_items_path do %>
              <div class='box-content box-statistic'>
                <h3 class='title text-warning'><%= @wait_confirm %></h3>
                <small>待確認客製化訂單</small>
                <div class='text-warning icon-check align-right'></div>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <hr>
